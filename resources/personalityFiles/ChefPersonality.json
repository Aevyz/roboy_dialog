{
  "initialState": "Greetings",
  "states": [
    {
      "identifier": "Greeting",
      "implementation": "roboy.dialog.states.chefStates.PassiveGreetingState",
      "transitions": {
        "greetingDetected": "ChefIntro"
      }
    },
    {
      "identifier": "ChefIntro",
      "implementation": "roboy.dialog.states.chefStates.ChefIntroductionState",
      "transitions": {
        "retrieveProducts": "RetrieveProducts",
      }
    },
    {
      "identifier": "RetrieveProducts",
      "implementation": "roboy.dialog.states.chefStates.RetrieveProductsState",
      "transitions": {
        "offerRecipe": "QuestionAnswering"
      }
    },
    {
      "identifier": "ReciteRecipe",
      "implementation": "roboy.dialog.states.chefStates.ReciteRecipeState",
      "transitions": {
        "questionAnswering": "Farewell"
      }
    },
    {
      "identifier": "Farewell",
      "implementation": "roboy.dialog.states.chefStates.FarewellState",
      "transitions": {}
    }
  ]
}