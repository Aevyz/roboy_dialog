{
  "initialState": "Greetings",
  "states": [
    {
      "identifier": "Greetings",
      "implementation" : "roboy.newDialog.states.GreetingsState",
      "fallback" : "RandomAnswer",
      "transitions" : {
        "greetingDetected" : "Uzupis"
      }
    },
    {
      "identifier": "Uzupis",
      "implementation" : "roboy.dialog.states.UzupisState",
      "fallback" : "RandomAnswer",
      "transitions" : {
        "loop": "Uzupis",
        "next": "Farewell"
      },
      "parameters" : {
        "QAFilePath" : "sentences/UzupisQAList.json"
      }
    },
    {
      "identifier": "Farewell",
      "implementation" : "roboy.dialog.examples.toyStates.ToyFarewellState",
      "transitions" : {}
    },
    {
      "identifier": "RandomAnswer",
      "implementation" : "roboy.dialog.examples.toyStates.ToyRandomAnswerState",
      "transitions" : {
        "next" : "Farewell"
      }
    }
  ]
}